{
    "version": 3.9,
    "builds": [
        {"src": "app.py", "use": "@vercel/python"}
    ],
    "routes": [
        {
            "src":"/(.*)", 
            "dest": "app.py",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"               
            ]
        }
    ]
}
